              end if
            end if

			//if globals(ErrChk) .ne. "1"
	        //    if trntype .sw. "AC"
	        //    else
	        //      set globals(ErrChk)="1"
	        //      error "Transaction Type mismatch<br>Use Acquisition Code only."
	        //    end if
			//end if

            if globals(ErrChk) .ne. "1"
              set myqry = "select xcode from fatrn where xvoucher='"+xvoucher+"'"
              set tmpchk = #sql(myqry)
              if #result .ne. "true"
                set globals(ErrChk)="1"
