                 end if
				end if
				
				
             	str mysql = "update famst set xlink = '"+xlink+"' where xcode='"+xcode+"'"
             	set tmpstr = #sql(str,mysql)
             	
             	str mysql = "update famst set xastname = '"+xastname+"' where xcode='"+xcode+"'"
