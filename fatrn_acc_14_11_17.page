             	if xasttype .ne. globals(xasttype)
             		//set globals(ErrChk)="1"
             		//error "Cannot Change Asset Sub Type"
             	end if
             end if

			 if globals(ErrChk) .ne. "1"
             	if xpurdate > xdate
             		set globals(ErrChk)="1"
