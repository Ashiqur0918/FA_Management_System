             	end if
             end if
             if globals(ErrChk) .ne. "1"
             
                set xlink="docs"+#sep+"x"+#id+#sep+xcode+".jpg"
				
				if globals(ErrChk) .ne. "1"                
                 int offset
                 int per
                 int year=#sub(xdate,0,4)

                 int tempper
                 int tempyear
                 set offset=castatus.xinteger("zid='"+#id+"' and xtypestatus='GL Period Offset'")
