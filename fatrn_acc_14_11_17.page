        field delete
          event before
            set globals(ErrChk) = ""
            set trntype=#sub (xvoucher,0,2)            
                        
            if globals(ErrChk) .ne. "1"
              if globals(xvoucher) .ne. xvoucher
                set globals(ErrChk) = "1"
